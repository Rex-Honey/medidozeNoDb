QWidget#mainWindow {
    background-color: lightblue;
}
QLineEdit[server=true] {
    background: #FFFFFF;
    border-radius: 10%;
    padding: 14px 10px;
    border: 1px solid #e1e4e6;
}

QLineEdit[serverError=true] {
    background: #FFFFFF;
    border-radius: 10%;
    padding: 14px 10px;
    border: 1px solid red;
}
QFrame#frame {
    background: #F4F7F9;
    border-radius: 10%;
    font-family: "Nirmala UI";
}