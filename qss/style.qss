QWidget#mainWindow {
    background-color: lightblue;
}

QFrame#frame {
    background: #F4F7F9;
    border-radius: 10%;
}

QLineEdit[ok=true] {
    background: #FFFFFF;
    border-radius: 10%;
    padding: 14px 10px;
    border: 1px solid #e1e4e6;
}

QLineEdit[error=true] {
    background: #FFFFFF;
    border-radius: 10%;
    padding: 14px 10px;
    border: 1px solid red;
}